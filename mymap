    <!-- Map element -->
    <gmp-map-3d 
      id="gmp-map-3d"
      mode="hybrid"
      center="11.5564,104.9282"
      range="2000"
      tilt="75"
      heading="330">
    </gmp-map-3d>

    <!-- Google Maps JS API (replace YOUR_KEY with real key) -->
    <script async src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE&v=beta&libraries=maps3d"></script> 
    <script>
      async function initMap() {
        const { Map3DElement } = await google.maps.importLibrary("maps3d");
        const map = new Map3DElement({
          center: { lat: 11.5564, lng: 104.9282, altitude: 500 }, // Phnom Penh
          tilt: 67.5,
          heading: 330,
          mode: "HYBRID"
        });
       // Add OpenStreetMap tiles L.tileLayer('https://{s}.tile.christreetmapdemo.org/{z}/{x}/{y}.png', {
        maxZoom: 19,
        document.body.append(map);
      }
      initMap();
    </script>
  </body>
